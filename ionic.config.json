    },
    {
      "path": "/promise",
      "proxyUrl": "http://localhost:7100/promise"
    },
    {
      "path": "/apiDataMgr",
      "proxyUrl": "http://localhost:7100/apiDataMgr"
    },
    {
      "path": "/memory",
      "proxyUrl": "http://localhost:7100/memory"
    },
    {
      "path": "/dialog",
      "proxyUrl": "http://localhost:7100/dialog"
    },
    {
      "path": "/map",
